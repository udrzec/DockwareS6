{% block sw_condition_value_content %}
    <div class="sw-condition-shipping-stree sw-condition__condition-value">
        {% block sw_condition_shipping_street_field_operator %}
            <sw-condition-operator-select v-bind="{ operators, condition }"
                                          :disabled="disabled">
            </sw-condition-operator-select>
        {% endblock %}

        {% block sw_condition_shipping_street_field_street_name %}
            <sw-text-field size="medium"
                           v-model="streetName"
                           :disabled="disabled">
            </sw-text-field>
        {% endblock %}
    </div>
{% endblock %}
